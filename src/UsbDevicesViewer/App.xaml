<Application x:Class="UsbDevicesViewer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml"
    xmlns:vurdalakov="clr-namespace:Vurdalakov">

    <Application.Resources>
        <vurdalakov:IntegerToObjectConverter Value0="Connected" Value1="Disconnected" Value2="Modified" x:Key="IntegerToWmiEventTypeConverter" />
        <vurdalakov:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    </Application.Resources>
    
</Application>
