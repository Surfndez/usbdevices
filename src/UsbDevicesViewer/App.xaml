<Application x:Class="UsbDevicesViewer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="MainWindow.xaml"
    xmlns:vurdalakov="clr-namespace:Vurdalakov">

    <Application.Resources>
        <vurdalakov:IntegerToObjectConverter Value0="Connected (WMI)" Value1="Disconnected (WMI)" Value2="Modified (WMI)" Value3="Connected" Value4="Disconnected" x:Key="IntegerToDeviceEventTypeConverter" />
        <vurdalakov:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        <vurdalakov:BooleanToVisibilityConverter Reverse="True" x:Key="BooleanToVisibilityReverseConverter" />
    </Application.Resources>
    
</Application>
